# 应用模式配置
REACT_APP_MODE=online  # online/offline 在此处切换模式
REACT_APP_API_BASE_URL=http://localhost:8080/api

# 离线模式配置
REACT_APP_OFFLINE_STORAGE_TYPE=indexedDB  # localStorage/indexedDB/fileSystem
REACT_APP_OFFLINE_DB_NAME=workflow_offline_db
REACT_APP_OFFLINE_AUTO_SAVE=true
REACT_APP_OFFLINE_SYNC_ONLINE=true

# 在线模式配置
REACT_APP_ONLINE_API_TIMEOUT=30000
REACT_APP_ONLINE_REALTIME_UPDATE=true